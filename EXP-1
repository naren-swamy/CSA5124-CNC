#include <stdio.h>

int main() {
    char *str = "Hello World";
    int i = 0;

    printf("Original string : %s\n", str);

    printf("Result string   : ");
    while (str[i] != '\0') {
        char result = str[i] ^ 0;   // XOR with 0
        printf("%c", result);
        i++;
    }

    printf("\n");

    return 0;
}

output:
Original string : Hello World
Result string   : Hello World

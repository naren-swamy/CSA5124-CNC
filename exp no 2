#include <stdio.h>

int main() {
    char *str = "Hello World";
    int i = 0;

    printf("Original string : %s\n\n", str);

    /* AND with 127 */
    printf("After AND with 127 : ");
    i = 0;
    while (str[i] != '\0') {
        char andResult = str[i] & 127;
        printf("%c", andResult);
        i++;
    }
    printf("\n");

    /* XOR with 127 */
    printf("After XOR with 127 : ");
    i = 0;
    while (str[i] != '\0') {
        char xorResult = str[i] ^ 127;
        printf("%c", xorResult);
        i++;
    }
    printf("\n");

    return 0;
}
output:
Original string : Hello World

After AND with 127 : Hello World
After XOR with 127 : 7
